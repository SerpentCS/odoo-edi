<odoo>
    <data>
        <record forcecreate="True" id="ir_cron_calendar_schedule_reminder" model="ir.cron">
            <field name="name">Calendar Schedule Reminder</field>
            <field name="model_id" ref="model_calendar_appointment"/>
            <field name="state">code</field>
            <field name="code">model.send_out_message()</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
        </record>
    </data>
</odoo>